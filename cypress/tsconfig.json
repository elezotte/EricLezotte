{
  "compilerOptions": {
    "jsx": "react",
    "allowJs": true,
    "strict": true,
    "target": "es6",
    "esModuleInterop": true,
    "noEmit": true,
    "lib": ["es5", "dom"],
    "types": ["cypress", "@testing-library/cypress"],
    "baseUrl": "..",
    "paths": {
      "fixtures/*": ["cypress/fixtures/*"],
      "lib/*": ["lib/*"],
      "src/*": ["src/*"]
    },
    "moduleResolution": "node"
  },
  "typeRoots": ["./typings"]
}
